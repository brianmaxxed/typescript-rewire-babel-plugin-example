{
  "compilerOptions": {
    "moduleResolution": "node",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonJS",
    "target": "es2015",
    "allowJs": true,
    "baseUrl": "./"
  }
}
